   "source": [
    "def get_prime_factors(n):\n",
    "    i = 2\n",
    "    while n > i:\n",
    "        while n%i == 0:\n",
    "            yield i\n",
    "            n //= i\n",
    "        i += 1\n",
    "            \n",
    "    if n > 1:\n",
    "        yield n\n",
    "\n",
    "list(get_prime_factors(100))"
   ]
